{
  "kind": "collectionType",
  "connection": "default",
  "collectionName": "articles",
  "info": {
    "name": "articles"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "image": {
      "model": "file",
      "via": "related",
      "plugin": "upload",
      "required": true
    },
    "avatar": {
      "model": "file",
      "via": "related",
      "plugin": "upload",
      "required": true
    },
    "excerpt": {
      "type": "richtext",
      "required": true
    },
    "content": {
      "type": "richtext",
      "required": true
    },
    "published": {
      "type": "date",
      "required": true
    },
    "slug": {
      "type": "string",
      "required": true
    },
    "category": {
      "model": "category"
    },
    "user": {
      "plugin": "users-permissions",
      "model": "user"
    },
    "category_slug": {
      "type": "string",
      "required": true
    }
  }
}
